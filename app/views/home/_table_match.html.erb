<% unless @match_requests.empty? %>
<table class="table table-hover match_routes" summary="User match_relations">
	<thead>
		<tr>
			<th>ID</th>
			<th>起点</th>
			<th>终点</th>
			<th>lng_s</th>
			<th>lng_d</th>
			<th>匹配结果</th>
			<th>创建时间</th>
		</tr>
	</thead>
	<tbody>
		<% @match_requests.each do |match| %>
		<tr>
			<td class="id"><%= match.id %></td>
			<td class="from"><%= match.from %></td>
			<td class="to"><%= match.to %></td>
			<td class=""><%= match.lat_s %></td>
			<td class=""><%= match.lat_d %></td>
			<td class=""><%= link_to '->', match, :remote => true %></td>
			<td><%= link_to 'Del', match, method: :delete %></td>
			<td class="created"><%= time_ago_in_words(match.created_at) %> ago</td>
		</tr>
		<% end %>
	</tbody>
</table>
<% end %>